{
  "name": "mongoose-ts-converter",
  "version": "1.0.3",
  "description": "A library that converts Mongoose models from Node.js codebase to TypeScript schema",
  "main": "cli/cli.js",
  "bin": {
    "mongoose-ts-converter": "./cli/cli.js"
  },
  "scripts": {
    "start": "node cli/cli.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "directories": {
    "lib": "lib"
  },
  "keywords": [
    "mongoose",
    "typescript",
    "schema",
    "converter"
  ],
  "author": "Franklyn Edekobi <edekobifrank@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "commander": "^10.0.0",
    "json-schema-to-typescript": "^15.0.2",
    "mongoose": "^8.7.2"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "jest-environment-node": "^29.7.0"
  }
}
